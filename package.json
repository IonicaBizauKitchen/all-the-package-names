    "type": "git",
    "url": "git+https://github.com/zeke/all-the-package-names.git"
  },
  "keywords": [
    "npm",
    "packages",
    "names",
    "list",
    "words"
  ],
  "author": "zeke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zeke/all-the-package-names/issues"
  },
  "homepage": "https://github.com/zeke/all-the-package-names#readme",
  "dependencies": {
    "lodash": "^3.10.0",
    "superagent": "^1.2.0"
  }
}
